.videos
    .videos__wrap
        each video in videos
            .videos__card
                iframe(width="100%", class="videos__thumbnail", src=`${video.url}`, frameborder="0", allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture", allowfullscreen)
                //- img(src=`${imagePath}videos-thumbnail.png`, class="videos__thumbnail", alt="video thumbnail")
                .videos__description
                    h3.videos__title= `${video.title}`
                    p.videos__caption= `${video.caption}`
        .videos__youtube
            h2 Conoce m√°s historias
            img(src=`${imagePath}icon-youtube-alt.svg`, alt="youtube")